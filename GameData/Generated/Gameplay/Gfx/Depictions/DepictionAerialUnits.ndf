    AdditionalTextures = $/M3D/Shader/HelicoDestTextures
    Selector = SpecificHelicoDepictionSelector
    CoatingName = 'Mi_24V_AT_TCH'
    Alternatives = Alternatives_Mi_24V_AT_TCH

    Operators = 
        [
            $/GFX/Sound/DepictionOperator_MovementSound_SM_Helico_Hind,
            DepictionOperator_SoundProbe,
            DepictionOperator_Turret_1_Aim,
            Op_Mi_24V_AT_TCH_Weapon1,
            Op_Mi_24V_AT_TCH_Weapon2,
            Op_Mi_24V_AT_TCH_Weapon3,
            DepictionOperator_CriticalEffects,
            DepictionOperator_HelicopterLanding,
            DepictionOperator_GroundPuff_Helico,
            DepictionOperator_ShadowPointCloudProvider,
            DepictionOperator_Rotors
            (
                HelixList = 
                [
                    THelix
                    (
                        BladeCount = 5
                        BladesBoneName = "helice_ls_1"
                        BlurActionId = "FX_Helice_1"
                        Clockwise = False
                        HelixBoneName = "bloc_moteur_1"
                        RotationAxis = 2
                        RotationSpeed = 242
                    ),
                    THelix
                    (
                        BladeCount = 3
                        BladesBoneName = "helice_ls_2"
                        BlurActionId = "FX_Helice_2"
                        Clockwise = False
                        HelixBoneName = "bloc_moteur_2"
                        RotationAxis = 1
                        RotationSpeed = 242
                    ),
                ]
            ),
            DepictionOperator_Feedback_Degat_Level1,
            DepictionOperator_Feedback_Degat_Level2,
            DepictionOperator_Heat,
            DepictionOperator_TurretCannonGatling_SlowRotation( XAxisHasTargetPhysicalPropertyName="tourelle1target" XAxisRotationNode="canon_01" ),
            DepictionOperator_EjectableProps_Helico,
        ]
        Actions = MAP[
                    ( "weapon_effet_tag1", Weapon_Gatling_JakB_12_7mm ),
                    ( "weapon_effet_tag2", Weapon_AGM_9M114M_KokonM_x8 ),
                    ( "weapon_effet_tag3", Weapon_RocketAir_B8_80mm_x10 ),
                ]
                + DepictionAction_Stress_And_Wrecked_Helicopter
                + MAP [ ('FX_Helice_1', Template_DepictionAction_Rotor( PaleLength = 2000 PaleCount = 5 RotationAxis = float3[0, 0, 1] SousMobile = "bloc_moteur_1" ))]
                + MAP [ ('FX_Helice_2', Template_DepictionAction_Rotor( PaleLength = 450 PaleCount = 3 RotationAxis = float3[0, 1, 0] SousMobile = "bloc_moteur_2" ))]
                + DepictionAction_CriticalFX_Helicopter

        SubDepictions = 

        [
            TSubDepiction
            (
                Anchors = 
                [
                    "pilot",
                    "pilot2",
                ]
                Depiction = Pilot_Pilote_Helico_SOV
            ),
        ]        

        SubDepictionGenerators = [ SubGenerators_Mi_24V_AT_TCH ]
)
